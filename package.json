  "version": "2.4.0-dev.20191124",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "api": "typedoc src --plugin typedoc-plugin-external-module-name --exclude \"**/+(test|benchmark)/**\" --out ",
    "benchmark": "node benchmark",
    "build": "npm run module && npm run compile && npm run test",
    "clean": "node build/clean",
    "compile": "tsc",
    "module": "tsc -p module.json",
    "dev": "tsc --watch",
    "test": "node test"
  },
  "devDependencies": {
    "@types/node": "^11.13.14",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.7.2"
  },
  "homepage": "https://github.com/samchon/tstl",
  "repository": {
    "type": "git",
